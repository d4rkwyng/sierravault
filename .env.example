# SierraVault API Keys
# Copy this file to .env and fill in your keys

# Required for LLM scoring
export ANTHROPIC_API_KEY=sk-ant-...
export OPENAI_API_KEY=sk-proj-...

# Required for research pipeline
export KAGI_API_KEY=
export BRAVE_API_KEY=
export SCRAPER_API_KEY=
export MOBYGAMES_API_KEY=

# Optional — path to Proton Drive Assets/sierravault repo (for research scripts)
# Defaults to ../Proton Drive Assets/sierravault if not set
export SIERRAVAULT_INTERNAL=

# Optional — Ollama Mac Studio hosts (for local LLM scoring)
export OLLAMA_STUDIO_TAILSCALE=http://your-tailscale-ip:11434
export OLLAMA_STUDIO_LOCAL=http://your-local-ip:11434
